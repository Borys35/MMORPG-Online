generator client {
  provider      = "prisma-client-js"
  output        = "../generated/prisma"
  binaryTargets = ["linux-musl-openssl-3.0.x", "native"]
}

datasource db {
  provider = "postgresql"
  // url      = env("DATABASE_URL")
  url      = "postgresql://borys:secret@localhost:5432/mmorpg_postgres?schema=public"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  hash      String
  username  String?
  createdAt DateTime @default(now())
}
